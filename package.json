{
  "name": "jmodal",
  "description": "A simple modal component for React",
  "author": "Julien Marcel",
  "keywords": ["react", "components", "ui", "modal"],
  "version": "1.0.7",
  "private": false,
  "main": "dist/index.js",
  "module": "src/index.js",
  "files": [ "dist", "README.md" ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jujunantes/JModal"
},
"bugs": {
  "url": "https://github.com/jujunantes/JModa/issues"
},
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production babel src/lib --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,__snapshots__",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.12.1",
    "cross-env": "^7.0.3",
    "rimraf": "^3.0.2",
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10"
  }
}
