{
  "name": "jmodal",
  "description": "Two test React components",
  "author": "Julien Marcel",
  "keywords": ["react", "components", "ui", "modal"],
  "version": "1.0.0",
  "private": false,
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "cross-env": "^7.0.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-scripts": "5.0.1",
    "rimraf": "^3.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "rimraf dist",
